{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": "off",
    "react/prop-types": "off",
    "react/display-name": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-unused-vars": "off",
    "no-debugger": "off"
  }
}
